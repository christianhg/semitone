{
  "name": "semitone",
  "version": "0.2.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/semitone.cjs",
  "module": "./dist/semitone.js",
  "types": "./dist/semitone.d.ts",
  "scripts": {
    "build": "tsup src/semitone.ts --format esm,cjs --dts --minify --sourcemap",
    "dev": "tsup src/semitone --format esm,cjs --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prettier": "prettier --write .",
    "setup-sandbox": "npm run build && cp ./dist/semitone.js ./sandbox/ && cp ./dist/semitone.js.map ./sandbox/"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  }
}
